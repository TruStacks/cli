{
	"build": {
			"args": {
					"VARIANT": "1.18-bullseye"
			}
	},
	"dockerComposeFile": "docker-compose.yml",
	"service": "devcontainer",
	"runServices": [
			"buildkitd",
			"postgresql",
			"temporal",
			"temporal-admin-tools",
			"temporal-ui",
			"loki",
			"promtail",
			"grafana",
			"minio"
	],
	"workspaceFolder": "/workspace",
	"shutdownAction": "none",
	"remoteUser": "vscode",
	"customizations": {
		"vscode": {
			"settings": {
				"k3d.paths": {
					"k3d.paths.k3d-linux": "/usr/local/bin/k3d"
				},
				"vs-kubernetes": {
					"vscode-kubernetes.kubectl-path.linux": "/usr/local/bin/kubectl",
					"vscode-kubernetes.helm-path.linux": "/usr/local/bin/helm"
				},
				"go.buildFlags": ["-tags=integration"]
			},
			"extensions": [
				"inercia.vscode-k3d",
				"ms-kubernetes-tools.vscode-kubernetes-tools"
			]
		}
	},
	"postStartCommand": "post-start"
}