{
	"build": {
			"args": {
					"VARIANT": "1.18-bullseye"
			}
	},
	"dockerComposeFile": "docker-compose.yml",
	"service": "devcontainer",
	"runServices": [
			"buildkitd",
			"postgresql",
			"temporal",
			"temporal-admin-tools",
			"temporal-ui",
			"loki",
			"promtail",
			"grafana"
	],
	"workspaceFolder": "/workspace",
	"shutdownAction": "none",
	"remoteUser": "vscode"
}